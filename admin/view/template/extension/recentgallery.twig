<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title"><i class="fa fa-rss"></i> {{ heading_title }}</h3>
  </div>
    <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <td>{{ column_galleryid }}</td>
          <td>{{ column_galleryname }}</td>
          <td>{{ column_totalalbum }}</td>
          <td>{{ column_status }}</td>
          <td class="text-right">{{ column_action }}</td>
        </tr>
      </thead>
      <tbody>
	    {% if recentgallerys %}
	  {% for gallerys in recentgallerys %}
        <tr>
          <td>{{ gallerys.album_id }}</td>
           <td>{{ gallerys.name }}</td>
			<td>{{ gallerys.total }}</td>
            <td>{{ gallerys.status }}</td>
		   <td class="text-right"><a href="{{ gallerys.href }}" data-toggle="tooltip" title="{{ button_view }}" ><i class="fa fa-eye"></i></a></td>
        </tr>
		{% endfor %}
		{% else %}
        <tr>
          <td class="text-center" colspan="6">{{ text_no_results }}</td>
        </tr>
		{% endif %}
      </tbody>
    </table>
  </div>
</div>